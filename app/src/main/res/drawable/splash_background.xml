<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <item>
        <shape>
            <gradient
                android:centerX="0.52"
                android:centerY="0.3"
                android:endColor="#FF9100"
                android:centerColor="@color/white"
                android:gradientRadius="300dp"
                android:startColor="#73FF00"
                android:angle="90"
                android:type="linear"/>
        </shape>
    </item>




    <item
        android:height="@dimen/_80sdp"
        android:width="@dimen/_80sdp"
        android:drawable="@drawable/ic_logo"
        android:gravity="center"/>
    <item
        android:left="@dimen/_60sdp"
        android:right="@dimen/_60sdp"
        android:bottom="@dimen/_60sdp"
        android:gravity="bottom"
        android:height="@dimen/_40sdp"
        android:drawable="@drawable/info_img">
    </item>


</layer-list>